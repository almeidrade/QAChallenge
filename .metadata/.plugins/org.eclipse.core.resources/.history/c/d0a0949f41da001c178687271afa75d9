package steps;

import java.util.concurrent.TimeUnit;
import io.cucumber.java.en.When;
import pages.HomePage;

public class WhenSteps {
	@When("the user navigate to the login page")
	public void the_user_navigate_to_the_login_page() throws InterruptedException, Throwable {
		TimeUnit.SECONDS.sleep(2); //added the explicitly sleeping in order to turn it better viewing
		HomePage.ClickContactFormPage();
	}
	
	@When("the user navigate to the Contact form page")
	public void the_user_navigate_to_the_contact_form_page() {
		
	}
}
